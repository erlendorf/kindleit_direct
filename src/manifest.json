{
    "name": "kindleit direct",
    "description": "Post webpage to kindle directly by using kindleit",
    "id": "ljjpaogbkhfbmbcamjdchjjfpcopjafm",
    "manifest_version": 2,
    "version": "0.0.7",
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs",
        "http://fivefilters.org/kindle-it",
        "notifications",
        "contextMenus"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png"
     },
    "options_page": "options.html",
    "background": {
        "scripts": ["jquery-1.6.1.min.js", "notifier.js", "background.js"]
    }
}
